{
    "id": "voipgrid@jetpack",
    "name": "voipgrid",
    "main": "./lib/main.js",
    "version": "1.6.10",
    "author": "VoIPGRID",
    "license": "MPL 1.1/GPL 2.0/LGPL 2.1",
    "fullName": "VoIPGRID",

    "description": "Firefox add-on for click-to-dial, status updates and queue monitoring.",

    "locales": {
        "en-US": {
            "title": "VoIPGRID",
            "description": "Firefox add-on for click-to-dial, status updates and queue monitoring."
        },
        "nl-NL": {
            "title": "VoIPGRID",
            "description": "Firefox plugin voor klik&bel, statusupdate en wachtrij monitoring."
        }
    },

    "preferences": [
        {
            "type": "string",
            "name": "platform_url",
            "value": "https://partner.voipgrid.nl/",
            "title": "platform_url_title",
            "description": "platform_url_description"
        },
        {
            "type": "bool",
            "name": "click_to_dial_enabled",
            "value": true,
            "title": "click_to_dial_enabled_title",
            "description": "click_to_dial_enabled_description"
        }
    ],
    "permissions": {
        "private-browsing": true,
        "multiprocess": true
    }
}
